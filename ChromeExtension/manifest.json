{
    "manifest_version": 2,
    "name": "Deemos",
    "description": "A youtube.com extension that allows you to vote on which video to watch next",
    "version": "1.0",
    "background": {
        "scripts":["js/background.js"],
        "persistent": true
    },
	"browser_action": {
		"default_title":"Youtube Extension",
        "default_icon":"Deemos_favicon.png",
        "default_popup": "popup.html"
	},
	"permissions":[
	    "activeTab"
	]
}

